<div class="container">
  <div class= "panel panel-default panel-heading">
    <h1  class="text-center">Currículum Vitae</h1>
    <ul class="nav nav-tabs">
      <li><a class="tablinks" onclick="openSector(event, 'Academico')" id="defaultOpen">Académico</a></li>
      <li><a class="tablinks" onclick="openSector(event, 'Laboral')">Laboral</a></li>
      <li><a class="tablinks" onclick="openSector(event, 'Seminarios')">Seminarios</a></li>
      <li><a class="tablinks" onclick="openSector(event, 'Publicaciones')">Publicaciones</a></li>
      <li><a class="tablinks" onclick="openSector(event, 'Meritos')">Meritos</a></li>
      <li><a class="tablinks" onclick="openSector(event, 'Membresias')">Membresías</a></li>
      <li><a class="tablinks" onclick="openSector(event, 'Conocimientos')">Conocimientos</a></li>
      <li><a class="tablinks" onclick="openSector(event, 'Habilidades')">Habilidades</a></li>
      <li><a class="tablinks" onclick="openSector(event, 'Idiomas')">Idiomas</a></li>
      <li><a class="tablinks" onclick="openSector(event, 'Referencias')">Referencias</a></li>
    </ul>
  </div>
  <!-- Sector Academico -->
  <div id='Academico' class="tabcontent">
    <div class="panel panel-default">
      <div class="panel-heading"><div class="panel-title"><h3>Formación Académica </h3></div></div>
        <div class="panel-body">
          <% if @educations.size >0%>
          <ul class="list-group">
            <% @educations.each do |education| %>
              <li class="list-group-item">
                <div>
                  <h4>
                    <b><%=education.title%>&emsp;</b>
                    <div class="btn-group pull-right">
                      <button class="btn btn-danger btn-sm  pull-right"><%=link_to 'Eliminar', education , method: :delete , data: { confirm: "Estas seguro de que quieres eliminar esta información?" }, :class=>"button-link"%></button>
                    <button class="btn btn-warning btn-sm pull-right" style="position:relative;"><%=link_to "Editar", [:edit,education], :class=>"button-link"%></button>
                  </div>
                  </h4>
                  <%=education.school_name%>&nbsp;
                  <%=education.start_date.year%>&emsp; - &emsp; <%=education.end_date.year%>
                </div>
              </li>
            <%end%>
          <%end%>
        </ul>
      </div>
    </div>



    <div class="panel-footer well">

      <%=render :partial=>'educations/form'%>

    </div>
  </div>

<!-- Sector Laboral -->
<div id='Laboral' class="tabcontent">
  <div class="panel panel-default">
    <div class="panel-heading">
      <div class="panel-title">
        <h3>Experiencia Profesional </h3>
      </div>
    </div>
    <div class="panel-body">
      <% if @experiences.size >0%>
        <ul class="list-group">
          <% @experiences.each do |experience| %>
          <li class="list-group-item">
          <div>
            <p>
              <h5><b><%=experience.company_name%></b></h5>
              <%=experience.job_title%>&emsp;
              <button class="btn btn-danger btn-sm  pull-right"><%=link_to 'Eliminar', experience , method: :delete , data: { confirm: "Estas seguro de que quieres eliminar esta información?" }, :class=>"button-link"%></button>
              <button class="btn btn-warning btn-sm pull-right" style="position:relative;"><%=link_to "Editar", [:edit,experience], :class=>"button-link"%></button>

              <br>
              <%=experience.start_job_date%>&emsp; - &emsp;
              <%=experience.finish_job_date%>
              <div>RANGO SALARIAL: <%=experience.salary_range%></div>
            </p>
          </div>
          </li>
          <%end%>
        </ul>
      <%end%>
    </div>
  </div>
  <div class="panel-footer well">
    <%= form_for :experience, url: '/experience/new' do |f| %>
    <div class="field col col-md-4">
      <%= f.label :cargo_profesional %>:<br />
      <%= f.text_field :job_title,:id=>'cargo',:class=>"form-control"%>
      <br>
      <%= f.label :empresa %>:<br />
      <%= f.text_field :company_name,:id=>'empresa',:class=>"form-control"%>
      <br>
    </div>
    <div class="field col col-md-4">
      <%= f.label :actividades_a_cargo %>:<br />
      <%= f.text_field :job_description,:class=>"form-control"%><br>
      <%= f.label :personas_bajo_mi_cargo %>:<br />
      <%= f.number_field :in_my_charge,:class=>"form-control"%>
    </div>
    <div class="field col col-md-4">
      <input type="checkbox" id="checkbox" name="checkbox">Desea agregar una persona de referencia
      <div id="rest" style="display:none;">
        <div class="field">
          <%= f.label :Persona_de_Referencia %>:<br />
          <%= f.text_field :referential,:class=>"form-control"%>
        </div>
        <div class="field">
          <%= f.label :numero_de_referencia %>:
          <%= f.number_field :referential_number,:class=>"form-control"%>
        </div>
      </div>
    </div>
    <div class="field">
      &nbsp;&nbsp;&nbsp;&nbsp;
    </div>
    <div class="field pull-right" style="position:relative;">
      ESTA INFORMACION NO ES PUBLICA
      <%= f.label :rango_salarial %>
      <%= f.select(:salary_range, options_for_select(Experience.salary_range)) %>
    </div>
    <div class="field">
      &nbsp;&nbsp;&nbsp;&nbsp;
      <%= f.label :Fecha_ingreso %>
      <%= f.date_select :start_date ,:start_year  =>1990, :end_year=>Time.now.year%>
    </div>
    <div class="field" id="date" style="display:none;">
      &nbsp;&nbsp;&nbsp;&nbsp;
      <%= f.label :fecha_salida %>
      <%= f.date_select :end_date ,:start_year  =>1990, :end_year=>Time.now.year%>
    </div>
    <div class="field">
      &nbsp;&nbsp;&nbsp;&nbsp;
      <%=f.label :si_aún_sigue_por_favor_deje_marcada_esta_casilla%>
      <input type="checkbox" id="checkboxDate" name="until_now", checked="true">
    </div>
      <%= f.hidden_field :resume_id, :value => current_user.resume.id%>
    <div class="actions">
      <br>
      &nbsp;&nbsp;&nbsp;&nbsp;
      <br><br>
      <%= f.submit "Guardar", :class =>"btn btn-primary" ,:id=>'boton_laboral'%>
    </div>
    <% end %>
  </div>
</div>

<!-- sector Seminarios -->
<div id='Seminarios' class="tabcontent">
