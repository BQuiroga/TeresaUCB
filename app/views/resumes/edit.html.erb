<script>
$(document).ready(function(){
    $('#boton_academico').attr('disabled',true);
    $('#filtrar_i').keyup(function(){
        if($(this).val().length !=0)
          $('#boton_academico').attr('disabled', false);
        else
            $('#boton_academico').attr('disabled',true);
    })

    $('#boton_laboral').attr('disabled',true);
    $('#cargo').keyup(function(){
        if($(this).val().length !=0 )
          $('#boton_laboral').attr('disabled', false);
        else
            $('#boton_laboral').attr('disabled',true);
    })
    $('#boton_seminario').attr('disabled',true);
