                      <th style="text-align: center;">&nbsp;Trabajos en dicho rango&nbsp;</th>
                    </tr>
                    <%
                      @educations=Education.where("title ~* ?", "Bachiller")
                      @users=Array.new
                      if @educations
                        @educations.each do |edu|
                          @users=[edu.user] +@users
                        end
                        if @users.size>1
                          @users=@users.uniq
                        end
                      end
                      @experiences=Array.new
                      @jobs_time=Hash.new(0)
                      @users.each do |user|
                        @experiences=@experiences +[user.resume.experiences]
                      end
                      @experiences.flatten.each do |e|
                        if e.time_in_job==-1
                          @jobs_time["Continua trabajando"]+=1
                        else
                          @jobs_time[e.time_in_job] +=1
                        end
